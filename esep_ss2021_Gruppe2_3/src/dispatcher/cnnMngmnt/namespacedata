#pragma once

#include <sys/dispatch.h>

namespace dispatcher {
namespace cnnMngmnt {

struct header_t {
    _Uint16t type;
    _Uint16t subtype;
    _Int8t code;
    _Uint8t zero[3];
    union sigval value;
    _Int32t scoid;
};

using rcvid = int;
using chid = int;
using status_code = int;
using conid = int;

} /* namespace */
} /* namespace */

