#ifndef SRC_UTILS_CONNMANAGEMENT_NAMESPACEDATA_
#define SRC_UTILS_CONNMANAGEMENT_NAMESPACEDATA_

#include <sys/dispatch.h>

namespace connManagement {

struct header_t {
    _Uint16t                    type;
    _Uint16t                    subtype;
    _Int8t                      code;
    _Uint8t                     zero[3];
    union sigval                value;
    _Int32t                     scoid;
};

using rcvid = int;
using chid = int;
using status_code = int;
using conid = int;

} /* connManagement dispatcher */

#endif /* SRC_UTILS_CONNMANAGEMENT_NAMESPACEDATA_ */
